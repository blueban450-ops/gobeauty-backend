{
  "expo": {
    "name": "GoBeauty",
    "slug": "gobeauty",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ""
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.asnlweb1.gobeauty"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications",
      "expo-location"
    ],
    "extra": {
      "apiUrl": "http://192.168.10.7:4000/api",
      "eas": {
        "projectId": "8be0cf1a-5792-47dc-9243-365f86242439"
      }
    }
  }
}
