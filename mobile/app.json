{
  "expo": {
    "name": "GoBeauty",
    "slug": "gobeauty",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "y",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ""
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.asnlweb1.gobeauty"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications",
      "expo-location"
    ],
    "extra": {
      "apiUrl": "https://gobeauty-backend.onrender.com/api",
      "eas": {
        "projectId": "8be0cf1a-5792-47dc-9243-365f86242439"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8be0cf1a-5792-47dc-9243-365f86242439"
    }
  }
}
